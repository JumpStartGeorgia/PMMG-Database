<%- model_class = Call1 -%>
<div class="page-header">
  <h1>უახლესი დარეკვები</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>სახელი</th>
      <th>გვარი</th>
      <th>ოლქის სახელი</th>
      <th>უბნის ნომერი</th>
      <th>შექმნის თარიღი</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @call1s.each do |call1| %>
      <tr>
        <td><%= call1.fname %></td>
        <td><%= call1.lname %></td>
        <td><%= link_to call1.dname,call1_path(call1) %></td>
        <td><%= call1.pname %></td>
        <td><%=l call1.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_call1_path(call1), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      call1_path(call1),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_call1_path,
            :class => 'btn btn-primary' %>
