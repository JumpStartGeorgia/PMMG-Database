<%- model_class = Call1 -%>
<div class="page-header">
  <h1><%= t('call1.form.edit.title') %></h1>
</div>

<div>
  <div class="row">
    <div class="header span2">
        სახელი:
    </div>
    <div class="content span6">
      <%= @call1.fname %>
    </div>
  </div>
</div>
<div>
  <div class="row">
    <div class="header span2">
        გვარი:
