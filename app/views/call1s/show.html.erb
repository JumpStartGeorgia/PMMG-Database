<%- model_class = Call1 -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fname) %>:</strong></dt>
  <dd><%= @call1.fname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lname) %>:</strong></dt>
  <dd><%= @call1.lname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dname) %>:</strong></dt>
  <dd><%= @call1.dname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pname) %>:</strong></dt>
  <dd><%= @call1.pname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_label) %>:</strong></dt>
  <dd><%= @call1.c_label %></dd>
  <dt><strong><%= model_class.human_attribute_name(:d_label) %>:</strong></dt>
  <dd><%= @call1.d_label %></dd>
  <dt><strong><%= model_class.human_attribute_name(:e_label) %>:</strong></dt>
  <dd><%= @call1.e_label %></dd>
  <dt><strong><%= model_class.human_attribute_name(:f_label) %>:</strong></dt>
  <dd><%= @call1.f_label %></dd>
  <dt><strong><%= model_class.human_attribute_name(:g_label) %>:</strong></dt>
  <dd><%= @call1.g_label %></dd>
  <dt><strong><%= model_class.human_attribute_name(:h_label) %>:</strong></dt>
  <dd><%= @call1.h_label %></dd>
  <dt><strong><%= model_class.human_attribute_name(:k_label) %>:</strong></dt>
  <dd><%= @call1.k_label %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              call1s_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_call1_path(@call1), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              call1_path(@call1),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
