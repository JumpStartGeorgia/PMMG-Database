<%- model_class = Call1 -%>
<div class="page-header">
  <h1><%= t('call1.form.edit.title') %></h1>
</div>

<div class="row span9">
  <table class="table table-striped">
    <tbody>
      <tr>
        <td>სახელი</td>
        <td><%= @call1.fname %></td>
      </tr>
      <tr>
        <td>გვარი</td>
        <td><%= @call1.lname %></td>
      </tr>
      <tr>
        <td>ოლქი</td>
        <td><%= @call1.district.dname %></td>
      </tr>
      <tr>
        <td>უბანი</td>
        <td><%= @call1.precinct.pname %></td>
      </tr>
      <tr>
        <td>C</td>
        <td><%= yn_label(@call1.c_label) %></td>
      </tr>
      <tr>
        <td>D</td>
        <td><%= yn_label(@call1.d_label) %></td>
      </tr>
      <tr>
        <td>E</td>
        <td><%= yn_label(@call1.e_label) %></td>
      </tr>      
      <tr>
        <td>F</td>
        <td><%= yn_label(@call1.f_label) %></td>
      </tr>
      <tr>
        <td>G</td>
        <td><%= yn_label(@call1.g_label) %></td>
      </tr>
      <tr>
        <td>H</td>
        <td><%= yn_label(@call1.h_label) %></td>
      </tr>
      <tr>
        <td>K</td>
        <td><%= yn_label(@call1.k_label) %></td>
      </tr>
    </tbody>
  </table>
</div>

<dl class="dl-horizontal">
<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call1s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call1_path(@call1), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call1_path(@call1),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
