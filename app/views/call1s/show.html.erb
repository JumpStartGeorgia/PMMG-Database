<%- model_class = Call1 -%>
<div class="page-header">
  <h1><%= t('call1.form.edit.title') %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>სახელი:</strong></dt>
  <dd><%= @call1.fname %></dd>
  <dt><strong>გვარი:</strong></dt>
  <dd><%= @call1.lname %></dd>
  <dt><strong>ოლქი:</strong></dt>
  <dd><%= @call1.district_id %></dd>
  <dt><strong>უბანი:</strong></dt>
  <dd><%= @call1.precinct_id %></dd>
  <dt><strong>C:</strong></dt>
  <dd><%= if @call1.c_label == true then 'დიახ' else 'არა' end %></dd>
  <dt><strong>D:</strong></dt>
  <dd><%= @call1.d_label %></dd>
  <dt><strong>E:</strong></dt>
  <dd><%= @call1.e_label %></dd>
  <dt><strong>F:</strong></dt>
  <dd><%= @call1.f_label %></dd>
  <dt><strong>G:</strong></dt>
  <dd><%= if @call1.g_label == true then 'დიახ' else 'არა' end %></dd>
  <dt><strong>H:</strong></dt>
  <dd><%= if @call1.h_label == true then 'დიახ' else 'არა' end %></dd>
  <dt><strong>K:</strong></dt>
  <dd><%= if @call1.k_label == true then 'დიახ' else 'არა' end %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call1s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call1_path(@call1), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call1_path(@call1),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
