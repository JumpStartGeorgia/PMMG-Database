<%- model_class = Call6 -%>
<div class="page-header">
  <h1>მე-6 დარეკვა</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>სახელი</th>
      <th>გვარი</th>
      <th>ოლქის დასახლება</th>
      <th>უბნის ნომერი</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @call6s.each do |call6| %>
      <tr>
        <td><%= link_to call6.fname, call6_path(call6) %></td>
        <td><%= call6.lname %></td>
        <td><%= call6.district_id %></td>
        <td><%= call6.precinct_id %></td>
        <td>
          <%= link_to t('app.buttons.edit'),
                      edit_call6_path(call6), :class => 'btn btn-mini' %>
          <%= link_to t('app.buttons.delete', :default => t("helpers.links.destroy")),
                      call6_path(call6),
                      :method => :delete,
                      :data => { :confirm => t('app.buttons.confirm') },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
