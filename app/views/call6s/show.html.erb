<%- model_class = Call6 -%>
<div class="page-header">
  <h1>მე-6 დარეკვა</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>სახელი:</strong></dt>
  <dd><%= @call6.fname %></dd>
  <dt><strong>გვარი:</strong></dt>
  <dd><%= @call6.lname %></dd>
  <dt><strong>ოლქი:</strong></dt>
  <dd><%= @call6.district.dname %></dd>
  <dt><strong>უბანი:</strong></dt>
  <dd><%= @call6.precinct.pname %></dd>
  <dt><strong>A:</strong></dt>
  <dd><%= @call6.label_a %></dd>
  <dt><strong>B:</strong></dt>
  <dd><%= @call6.label_b %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call6s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call6_path(@call6), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call6_path(@call6),
              :method => 'delete',
              :data => { :confirm => t('app.buttons.confirm') },
              :class => 'btn btn-danger' %>
</div>
