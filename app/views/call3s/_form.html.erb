<%= form_for @call3, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.error_messages %>
  <%= field_set_tag '', :class=>"label" do %>
  <div class="control-group">
    <%= label :fname, t('call3.form.fname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= label :lname, t('call3.form.fname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :lname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= label :district_id, t('call3.form.dname'), :for=>"call3_district_id", :class=>"control-label" %>
    <div class="controls">
      <%= f.collection_select :district_id, District.order(:dname), :id, :dname, include_blank: true %>
    </div>
  </div>
  <div class="control-group">
    <%= label :precinct_id, t('call3.form.pname'), :for=>"call3_precinct_id", :class=>"control-label" %>
    <div class="controls">
      <%= f.grouped_collection_select :precinct_id, District.order(:dname), :precincts, :dname, :pname, :id, include_blank: true %>
    </div>
  </div>
  <% end %>
  <div class="control-group">
    <span class="label">M</span>
    <%= label :label_m, t('call3.form.label_m'), :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :label_m, :class => 'number_field' %>
    </div>
  </div>
  <div class="form-actions">
    <%= f.submit t('app.buttons.save'), :class => 'btn btn-primary' %>
    <%= link_to t('app.buttons.cancel'),
                call3s_path, :class => 'btn' %>
  </div>
<% end %>
