<%- model_class = Call3 -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fname) %>:</strong></dt>
  <dd><%= @call3.fname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lname) %>:</strong></dt>
  <dd><%= @call3.lname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dname) %>:</strong></dt>
  <dd><%= @call3.dname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pname) %>:</strong></dt>
  <dd><%= @call3.pname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:label_m) %>:</strong></dt>
  <dd><%= @call3.label_m %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              call3s_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_call3_path(@call3), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              call3_path(@call3),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
