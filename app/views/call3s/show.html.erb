<%- model_class = Call3 -%>
<div class="page-header">
  <h1>მე-3 დარეკვა</h1>
</div>

<div class="row span9">
  <table class="table table-striped">
    <tbody>
      <tr>
        <td>სახელი</td>
        <td><%= @call3.fname %></td>
      </tr>
      <tr>
        <td>გვარი</td>
        <td><%= @call3.lname %></td>
      </tr>
      <tr>
        <td>ოლქი</td>
        <td><%= @call3.district.dname %></td>
      </tr>
      <tr>
        <td>უბანი</td>
        <td><%= @call3.precinct.pname %></td>
      </tr>
      <tr>
        <td>N</td>
        <td><%= @call3.label_m %></td>
      </tr
    </tbody>
  </table>
</div>

<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call3s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call3_path(@call3), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call3_path(@call3),
              :method => 'delete',
              :data => { :confirm => t('app.buttons.confirm') },
              :class => 'btn btn-danger' %>
</div>
