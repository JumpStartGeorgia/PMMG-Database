<%- model_class = Call3 -%>
<div class="page-header">
  <h1>მე-3 დარეკვა</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>სახელი:</strong></dt>
  <dd><%= @call3.fname %></dd>
  <dt><strong>გვარი:</strong></dt>
  <dd><%= @call3.lname %></dd>
  <dt><strong>ოლქი:</strong></dt>
  <dd><%= @call3.dname %></dd>
  <dt><strong>უბანი:</strong></dt>
  <dd><%= @call3.pname %></dd>
  <dt><strong>M:</strong></dt>
  <dd><%= @call3.label_m %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call3s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call3_path(@call3), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call3_path(@call3),
              :method => 'delete',
              :data => { :confirm => t('app.buttons.confirm') },
              :class => 'btn btn-danger' %>
</div>
