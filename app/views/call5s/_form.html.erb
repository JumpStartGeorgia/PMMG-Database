<%= form_for @call5, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= label :fname, t('call5.form.fname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= label :lname, t('call5.form.lname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :lname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= label :dname, t('call5.form.dname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.collection_select :dname, District.order(:dname), :id, :dname, include_blank: true %>
    </div>
  </div>
  <div class="control-group">
    <%= label :pname, t('call5.form.pname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.grouped_collection_select :pname, District.order(:dname), :precincts, :dname, :id, :pname, include_blank: true %>
    </div>
  </div>
  <div class="control-group">
    <%= label :label_al, t('call5.form.label_al'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_al, true %>
      <%= label :label_al, t('call5.form.label_al_one') %>
      <%= f.radio_button :label_al, false %>
      <%= label :label_al, t('call5.form.label_al_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= label :label_am, t('call5.form.label_am'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_am, true %>
      <%= label :label_am, t('call5.form.label_am_one') %>
      <%= f.radio_button :label_am, false %>
      <%= label :label_am, t('call5.form.label_am_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= label :label_an, t('call5.form.label_an'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_an, true %>
      <%= label :label_an, t('call5.form.label_an_one') %>
      <%= f.radio_button :label_an, false %>
      <%= label :label_an, t('call5.form.label_an_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= label :label_ap, t('call5.form.label_ap'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_ap, true %>
      <%= label :label_ap, t('call5.form.label_ap_one') %>
      <%= f.radio_button :label_ap, false %>
      <%= label :label_ap, t('call5.form.label_ap_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= label :label_aq, t('call5.form.label_aq'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_aq, true %>
      <%= label :label_aq, t('call5.form.label_aq_one') %>
      <%= f.radio_button :label_aq, false %>
      <%= label :label_aq, t('call5.form.label_aq_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= label :label_ar, t('call5.form.label_ar'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_ar, true %>
      <%= label :label_ar, t('call5.form.label_ar_one') %>
      <%= f.radio_button :label_ar, false %>
      <%= label :label_ar, t('call5.form.label_ar_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :label_as, t('call5.form.label_as'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_as, true %>
      <%= label :label_as, t('call5.form.label_as_one') %>
      <%= f.radio_button :label_as, false %>
      <%= label :label_as, t('call5.form.label_as_two') %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :label_at, t('call5.form.label_at'), :class => 'control-label' %>
    <div class="controls">
      <%= f.radio_button :label_at, true %>
      <%= label :label_at, t('call5.form.label_at_one') %>
      <%= f.radio_button :label_at, false %>
      <%= label :label_at, t('call5.form.label_at_two') %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit t('app.buttons.save'), :class => 'btn btn-primary' %>
    <%= link_to t('app.buttons.cancel'),
                call5s_path, :class => 'btn' %>
  </div>
<% end %>
