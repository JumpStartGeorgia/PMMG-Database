<%= form_for @call5, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.error_messages %>
  <%= field_set_tag '', :class=>"label" do %>
  <div class="control-group">
    <%= label :fname, t('call5.form.fname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= label :lname, t('call5.form.lname'), :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :lname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= label :district_id, t('call5.form.dname'), :for=>"call5_district_id", :class=>"control-label" %>
    <div class="controls">
      <%= f.collection_select :district_id, District.order(:dname), :id, :dname, include_blank: true %>
    </div>
  </div>
  <div class="control-group">
    <%= label :precinct_id, t('call5.form.pname'), :for=>"call5_precinct_id", :class=>"control-label" %>
    <div class="controls">
      <%= f.grouped_collection_select :precinct_id, District.order(:dname), :precincts, :dname, :pname, :id, include_blank: true %>
    </div>
  </div>
  <% end %>  
  <div class="control-group">
    <span class="label">AL</span>
    <%= f.label :label_al, t('call5.form.label_al'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_al, true, :class=>'radio' %>
          <%= t('call5.form.label_al_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_al, false, :class=>'radio' %>
          <%= t('call5.form.label_al_two') %>  
        </label>
      </div>
  </div>
  <div class="control-group">
    <span class="label">AM</span>
    <%= f.label :label_am, t('call5.form.label_am'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_am, true, :class=>'radio' %>
          <%= t('call5.form.label_am_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_am, false, :class=>'radio' %>
          <%= t('call5.form.label_am_two') %>  
        </label>
      </div>
  </div>
  <div class="control-group">
    <span class="label">AN</span>
    <%= f.label :label_an, t('call5.form.label_an'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_an, true, :class=>'radio' %>
          <%= t('call5.form.label_an_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_an, false, :class=>'radio' %>
          <%= t('call5.form.label_an_two') %>  
        </label>
      </div>
  </div>
  <div class="control-group">
    <span class="label">AP</span>
    <%= f.label :label_ap, t('call5.form.label_ap'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_ap, true, :class=>'radio' %>
          <%= t('call5.form.label_ap_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_ap, false, :class=>'radio' %>
          <%= t('call5.form.label_ap_two') %>  
        </label>
      </div>
  </div>
  <div class="control-group">
    <span class="label">AQ</span>
    <%= f.label :label_aq, t('call5.form.label_aq'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_aq, true, :class=>'radio' %>
          <%= t('call5.form.label_aq_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_aq, false, :class=>'radio' %>
          <%= t('call5.form.label_aq_two') %>  
        </label>
      </div>
  </div>
  <div class="control-group">
    <span class="label">AR</span>
    <%= f.label :label_ar, t('call5.form.label_ar'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_ar, true, :class=>'radio' %>
          <%= t('call5.form.label_ar_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_ar, false, :class=>'radio' %>
          <%= t('call5.form.label_ar_two') %>  
        </label>
      </div>
  </div>
  <div class="control-group">
    <span class="label">AT</span>
    <%= f.label :label_at, t('call5.form.label_at'), :class=>'control-label' %>
      <div class="controls">
        <label class="radio">
          <%= f.radio_button :label_at, true, :class=>'radio' %>
          <%= t('call5.form.label_at_one') %>  
        </label>
        <label class="radio">
          <%= f.radio_button :label_at, false, :class=>'radio' %>
          <%= t('call5.form.label_at_two') %>  
        </label>
      </div>
  </div>
  <div class="form-actions">
    <%= f.submit t('app.buttons.save'), :class => 'btn btn-primary' %>
    <%= link_to t('app.buttons.cancel'),
                call5s_path, :class => 'btn' %>
  </div>
<% end %>
