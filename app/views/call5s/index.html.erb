<%- model_class = Call5 -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:fname) %></th>
      <th><%= model_class.human_attribute_name(:lname) %></th>
      <th><%= model_class.human_attribute_name(:dname) %></th>
      <th><%= model_class.human_attribute_name(:pname) %></th>
      <th><%= model_class.human_attribute_name(:label_al) %></th>
      <th><%= model_class.human_attribute_name(:label_am) %></th>
      <th><%= model_class.human_attribute_name(:label_an) %></th>
      <th><%= model_class.human_attribute_name(:label_ap) %></th>
      <th><%= model_class.human_attribute_name(:label_aq) %></th>
      <th><%= model_class.human_attribute_name(:label_ar) %></th>
      <th><%= model_class.human_attribute_name(:label_as) %></th>
      <th><%= model_class.human_attribute_name(:label_at) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @call5s.each do |call5| %>
      <tr>
        <td><%= link_to call5.id, call5_path(call5) %></td>
        <td><%= call5.fname %></td>
        <td><%= call5.lname %></td>
        <td><%= call5.dname %></td>
        <td><%= call5.pname %></td>
        <td><%= call5.label_al %></td>
        <td><%= call5.label_am %></td>
        <td><%= call5.label_an %></td>
        <td><%= call5.label_ap %></td>
        <td><%= call5.label_aq %></td>
        <td><%= call5.label_ar %></td>
        <td><%= call5.label_as %></td>
        <td><%= call5.label_at %></td>
        <td><%=l call5.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_call5_path(call5), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      call5_path(call5),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_call5_path,
            :class => 'btn btn-primary' %>
