<%- model_class = Call2 -%>
<div class="page-header">
  <h1><%=t 'call2.form.edit.title' %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>სახელი:</strong></dt>
  <dd><%= @call2.fname %></dd>
  <dt><strong>გვარი:</strong></dt>
  <dd><%= @call2.lname %></dd>
  <dt><strong>ოლქი:</strong></dt>
  <dd><%= @call2.dname %></dd>
  <dt><strong>უბანი:</strong></dt>
  <dd><%= @call2.pname %></dd>
  <dt><strong>L:</strong></dt>
  <dd><%= @call2.label_l %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call2s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call2_path(@call2), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call2_path(@call2),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
