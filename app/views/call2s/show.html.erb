<%- model_class = Call2 -%>
<div class="page-header">
  <h1><%=t 'call2.form.edit.title' %></h1>
</div>

<div class="row span9">
  <table class="table table-striped">
    <tbody>
      <tr>
        <td>სახელი</td>
        <td><%= @call2.fname %></td>
      </tr>
      <tr>
        <td>გვარი</td>
        <td><%= @call2.lname %></td>
      </tr>
      <tr>
        <td>ოლქი</td>
        <td><%= @call2.district.dname %></td>
      </tr>
      <tr>
        <td>უბანი</td>
        <td><%= @call2.precinct.pname %></td>
      </tr>
      <tr>
        <td>N</td>
        <td><%= @call2.label_l %></td>
      </tr
    </tbody>
  </table>
</div>

<div class="form-actions">
  <%= link_to t('app.buttons.back'),
              call2s_path, :class => 'btn'  %>
  <%= link_to t('app.buttons.edit'),
              edit_call2_path(@call2), :class => 'btn' %>
  <%= link_to t('app.buttons.delete'),
              call2_path(@call2),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
